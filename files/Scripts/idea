#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2021-10-14T05:36:04.140424

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/brekitomasson/Library/Application Support/JetBrains/Toolbox/apps/IDEA-C/ch-0/212.5284.40/IntelliJ IDEA CE.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"