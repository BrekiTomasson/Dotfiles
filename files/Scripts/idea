#!/bin/sh
#Generated by JetBrains Toolbox 1.20.8352 at 2021-05-16T14:45:57.266534

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/brekitomasson/Library/Application Support/JetBrains/Toolbox/apps/IDEA-C/ch-0/211.7142.45/IntelliJ IDEA CE.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"