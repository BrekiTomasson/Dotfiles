#!/bin/bash
#Generated by JetBrains Toolbox 2.0.1.16621 at 2023-08-23T18:42:47.911611

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/brekitomasson/Applications/IntelliJ IDEA Community Edition.app/Contents/MacOS/idea" $wait --args "${intellij_args[@]}"