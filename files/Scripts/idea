#!/bin/bash
#Generated by JetBrains Toolbox 1.27.0.13477 at 2022-11-26T23:37:09.859648

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/brekitomasson/Library/Application Support/JetBrains/Toolbox/apps/IDEA-C/ch-0/222.4459.24/IntelliJ IDEA CE.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"