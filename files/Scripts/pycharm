#!/bin/bash
#Generated by JetBrains Toolbox 1.22.10774 at 2021-12-01T15:35:10.115386

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/brekitomasson/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-C/ch-0/213.5744.248/PyCharm CE.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"