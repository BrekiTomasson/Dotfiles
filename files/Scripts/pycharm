#!/bin/bash
#Generated by JetBrains Toolbox 1.24.12080 at 2022-06-26T11:15:01.453958

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/brekitomasson/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-C/ch-0/221.5921.27/PyCharm CE.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"